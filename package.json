{
  "name": "luna-react-timeline",
  "private": true,
  "version": "0.1.0",
  "description": "Luna React Timeline (Gantt)",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf ./lib",
    "build:js": "babel src/ -d lib/",
    "watch:js": "babel -w src/ -d lib/",
    "build:css": "node-sass src/scss/style.scss lib/css/style.css",
    "watch:css": "node-sass -w src/scss/style.scss lib/css/style.css",
    "watch": "yarn js & yarn css",
    "build": "yarn run clean && yarn build:js && yarn build:css"
  },
  "repository": "git@github.com:JSainsburyPLC/luna-react-timeline-prototype.git",
  "author": "JSainsburyPLC",
  "license": "UNLICENSED",
  "dependencies": {},
  "peerDependencies": {
    "react": ">=15.0",
    "react-dom": ">=15.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "node-sass": "^4.5.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
